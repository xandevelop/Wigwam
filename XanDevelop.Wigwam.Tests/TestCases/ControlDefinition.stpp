control | name = Login Button 1 | Id=btnLogin | FriendlyName = Login Btn # Login Button specified by ID
control | name = Login Button 2 | css=#btnLogin | FriendlyName = Login Btn # Login button specified by CSS
control | name = Login Button 3 | xpath=//@id=btnLogin | FriendlyName = Login Btn # Login button specified by XPath

